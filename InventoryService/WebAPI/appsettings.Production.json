{
    "Serilog": {
        "Using": [
           "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ]
    },
    "AllowedHosts": "*",
    "URLS": {
        "AbsoluteUrl": "https://auth.com/auth"
    },
    "OIDC": {
        "Url": "https://inventoryservice-api.com",
        "RedirectUrl": "",
        "ClientId": "",
        "ResponseType": "id_token token",
        "Scope": "openid profile"
    },
    "MongoDbSettings": {
        "ConnectionString": "mongodb://localhost:27017/",
        "DatabaseName": "inventoryservice-api"
    }
}